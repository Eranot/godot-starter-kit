; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="StarterKit"
run/main_scene="res://scenes/menu/splash_screen/splash_screen.tscn"
config/features=PackedStringArray("4.0", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

SoundController="*res://global/sound_controller/sound_controller.tscn"

[display]

window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/max_size_container/plugin.cfg")

[rendering]

textures/canvas_textures/default_texture_filter=0
